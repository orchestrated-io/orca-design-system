{
  "name": "orca-design-system",
  "version": "1.1.15",
  "description": "Orchestrated Frontend React NPM library for Brand designs",
  "main": "dist/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint lib/",
    "start": "styleguidist server",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "build": "styleguidist build",
    "dist": "babel lib -d dist && npm run module",
    "module": "BABEL_ENV=es babel lib -d es",
    "deploy-stack": "serverless deploy",
    "deploy-client": "serverless client deploy  --no-confirm",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic",
    "chromatic:ci": "chromatic --exit-zero-on-changes",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@storybook/storybook-deployer": "^2.8.1",
    "polished": "^3.3.0",
    "prop-types": "^15.6.2",
    "react-dates": "^20.1.0",
    "react-number-format": "^4.3.1",
    "react-router": "^4.3.1",
    "react-select": "^1.2.1",
    "styled-is": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "@storybook/addon-a11y": "^5.2.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-docs": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-styleguidist": "^9.1.16",
    "serverless": "^1.55.1",
    "serverless-finch": "^2.4.3",
    "storybook-chromatic": "^3.0.3",
    "style-loader": "^1.0.0",
    "styled-components": "^5.0.1",
    "webpack": "^4.40.0",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {
    "react": ">=0.16",
    "react-dom": ">=0.16",
    "styled-components": "^5.0.1"
  },
  "files": [
    "dist",
    "es"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write lib/**/*.js* && npm run lint"
    }
  }
}
