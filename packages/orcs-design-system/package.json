{
  "name": "orcs-design-system",
  "version": "0.9.9",
  "description": "Orchestrated Design System: ORCS",
  "main": "es/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "audit-ci": "audit-ci -m --config ./audit-ci.json",
    "lint": "CI=true eslint --max-warnings=0 lib/",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "build": "styleguidist build",
    "dist": "BABEL_ENV=es babel lib -d es",
    "module": "BABEL_ENV=es babel lib -d es",
    "deploy-stack": "serverless deploy",
    "deploy-client": "serverless client deploy  --no-confirm",
    "storybook": "start-storybook -s ./lib/assets",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic",
    "chromatic:ci": "chromatic --exit-zero-on-changes",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "eslint-plugin-jest": "^23.8.2",
    "polished": "^3.4.4",
    "prop-types": "^15.6.2",
    "react-dates": "^21.8.0",
    "react-number-format": "^4.4.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "styled-components-grid": "^2.2.2",
    "styled-is": "^1.3.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/jest": "^25.2.1",
    "@types/styled-components": "^5.1.0",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^6.0.0-alpha.5",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/storybook-deployer": "^2.8.5",
    "audit-ci": "^2.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "serverless": "^1.66.0",
    "serverless-finch": "^2.5.2",
    "storybook-chromatic": "^3.5.2",
    "style-loader": "^1.1.3",
    "styled-components": "^4.4.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": ">=0.16",
    "react-dom": ">=0.16",
    "styled-components": "^4.4.0"
  },
  "files": [
    "es"
  ],
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/lib/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/lib/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "./installGitSecrets.sh &&  git secrets --register-aws && git-secrets --scan -r && npm run audit-ci",
      "pre-commit": "prettier --write lib/**/*.js* && npm run lint"
    }
  },
  "gitHead": "69f16f52a88aa7700b72e66285fec7ea49a516f8"
}
