{
  "extends": [
    "prettier",
    "prettier/react",
    "prettier/standard",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react", "prettier", "react-hooks"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
